{
  "extends": [
    "development"
  ],
  "hints": {
    "meta-viewport": "off",
    "axe/language": [
      "default",
      {
        "html-has-lang": "off"
      }
    ],
    "axe/text-alternatives": [
      "default",
      {
        "document-title": "off",
        "frame-title": "off"
      }
    ],
    "meta-charset-utf-8": "off",
    "no-inline-styles": "off",
    "button-type": "off",
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "iframe[loading]",
          "iframe[allow]"
        ]
      }
    ],
    "axe/keyboard": [
      "default",
      {
        "nested-interactive": "off"
      }
    ],
    "highest-available-document-mode": "off",
    "axe/name-role-value": "off"
  }
}